<?xml version="1.0"?>
<launch>

  <node pkg="kitti_velodyne_ros" type="kitti_velodyne_ros" name="kitti_velodyne_ros" output="screen" respawn="false">
    <param name="velodyne_dir" type="string" value="/home/epan/Rui/datasets/KITTI_Odometry/test/velodyne/"/>
    <param name="poses_file" type="string" value="/home/epan/Rui/datasets/KITTI_Odometry/test/poses.txt"/>
    <param name="times_file" type="string" value="/home/epan/Rui/datasets/KITTI_Odometry/test/times.txt"/>
    <param name="frequency" type="double" value="3"/>
    <remap from="/kitti_velodyne_ros/velodyne_points" to="/points_raw"/>
  </node>

  <node pkg="kitti_camera_ros" type="kitti_camera_ros" name="kitti_camera_ros" output="screen" respawn="false">
